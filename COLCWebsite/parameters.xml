<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<parameter name="DataCacheMinutes" description="Number of minutes to cache data for occupations, region details, locations, taxes, and salary levels">
		<parameterEntry kind="XmlFile" scope=".\Web.config" match="/configuration/appSettings/add[@key='DataCacheMinutes']/@value" />
	</parameter>
	<parameter name="WelcomeBCHost" description="Host for WelcomeBC skin" defaultValue="456">
		<parameterEntry kind="XmlFile" scope="iisapp/Web.config" match="/configuration/appSettings/add[@key='WelcomeBCHost']/@value" />
	</parameter>
	<parameter name="WorkBCHost" description="Host for WorkBC skin" defaultValue="123">
		<parameterEntry kind="XmlFile" scope="iisapp/COLCWebsite/Web.config" match="/configuration/appSettings/add[@key='WorkBCHost']/@value" />
	</parameter>
	<parameter name="CompilationDebugSetting" description="Debug compilation setting">
		<parameterEntry kind="XmlFile" scope=".\Web.config" match="/configuration/system.net/compilation/@debug" />
	</parameter>
	<parameter name="CustomErrorsMode" description="Custom errors mode setting, Values: Off, RemoteOnly, On">
		<parameterEntry kind="XmlFile" scope=".\Web.config" match="/configuration/system.net/customErrors/@mode" />
	</parameter>
</parameters>
